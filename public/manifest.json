{
  "name": "POE Stash History",
  "version": "0.1",
  "description": "A dashboard for Path Of Exile stash history",
  "permissions": [
    "cookies",
    "tabs",
    "http://web.poe.garena.tw/*",
    "https://web.poe.garena.tw/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "name": "POE Stash History",
    "default_icon": "stash.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'sha256-d6aTHtUx79YVQ+mmxbyuqVDutawku/ZN2azbAExrEd4=';  object-src 'self'",
  "manifest_version": 2
}
